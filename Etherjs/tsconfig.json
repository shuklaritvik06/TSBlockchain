{
  "compilerOptions": {
    "incremental": true,
    "target": "es2016",
    "lib": ["DOM", "ES2015", "ScriptHost"],
    "module": "commonjs",
    "allowJs": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipDefaultLibCheck": true,
    "skipLibCheck": true
  }
}
